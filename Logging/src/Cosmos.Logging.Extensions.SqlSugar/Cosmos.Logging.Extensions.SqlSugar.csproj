  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)'=='net451'">
    <PackageReference Include="sqlSugar" Version="4.6.4.7" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="sqlSugar" Version="4.6.4.7" />
  </ItemGroup>
</Project>